  RowBox[{
   RowBox[{
    RowBox[{"deleteZeroTerm", "[", "x_", "]"}], ":=", 
    RowBox[{"Module", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"xlist", ",", "k"}], "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"xlist", "=", "x"}], ";", "\[IndentingNewLine]", 
       RowBox[{"If", "[", " ", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{"Head", "[", "xlist", "]"}], "===", "Plus"}], ",", 
